{
  "name": "demosite",
  "description": "Application utilizing Sitecore JavaScript Services and Next.js",
  "version": "21.1.6",
  "private": true,
  "config": {
    "appName": "demosite",
    "rootPlaceholders": [
      "jss-main",
      "jss-header",
      "jss-breadcrumb",
      "jss-main-bottom",
      "jss-footer",
      "jss-main",
      "sxa-composites"
    ],
    "sitecoreConfigPath": "/App_Config/Include/zzz",
    "graphQLEndpointPath": "/sitecore/api/graph/edge",
    "language": "en",
    "templates": [
      "nextjs",
      "nextjs-sxa",
      "nextjs-styleguide",
      "nextjs-multisite"
    ]
  },
  "engines": {
    "node": ">=12",
    "npm": ">=6"
  },
  "author": {
    "name": "Sitecore Corporation",
    "url": "https://jss.sitecore.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sitecore/jss.git"
  },
  "bugs": {
    "url": "https://github.com/sitecore/jss/issues"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@sitecore-jss/sitecore-jss-nextjs": "~21.1.6",
    "@types/webpack": "^5.28.1",
    "bootstrap": "^5.2.3",
    "classnames": "^2.3.2",
    "dotenv-flow": "^3.2.0",
    "graphql": "~15.8.0",
    "graphql-tag": "^2.12.6",
    "next": "^13.1.6",
    "next-localization": "^0.12.0",
    "nprogress": "~0.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1",
    "sharp": "^0.31.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.21.8",
    "@graphql-codegen/import-types-preset": "^2.2.6",
    "@graphql-codegen/plugin-helpers": "^3.1.2",
    "@graphql-codegen/typed-document-node": "^2.3.12",
    "@graphql-codegen/typescript": "^2.8.7",
    "@graphql-codegen/typescript-operations": "^2.5.12",
    "@graphql-codegen/typescript-resolvers": "^2.7.12",
    "@graphql-typed-document-node/core": "^3.1.1",
    "@sitecore-jss/sitecore-jss-cli": "~21.1.6",
    "@sitecore-jss/sitecore-jss-dev-tools": "~21.1.6",
    "@storybook/addon-actions": "^7.0.18",
    "@storybook/addon-essentials": "^7.0.18",
    "@storybook/addon-interactions": "^7.0.18",
    "@storybook/addon-links": "^7.0.18",
    "@storybook/nextjs": "^7.0.18",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^7.0.18",
    "@storybook/testing-library": "^0.1.0",
    "@svgr/webpack": "^6.5.1",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^18.11.18",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "@types/webpack-env": "^1.18.1",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "chalk": "~4.1.2",
    "chokidar": "~3.5.3",
    "constant-case": "^3.0.4",
    "cross-env": "~7.0.3",
    "css-loader": "^6.7.3",
    "dotenv": "^16.0.3",
    "eslint": "^8.32.0",
    "eslint-config-next": "^13.1.5",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-yaml": "^0.5.0",
    "fs-extra": "^11.1.0",
    "graphql-let": "^0.18.6",
    "image-downloader": "^4.3.0",
    "npm-run-all": "~4.1.5",
    "plop": "^3.1.2",
    "prettier": "^2.8.3",
    "replace-in-file": "^6.3.5",
    "sass": "^1.57.1",
    "sass-alias": "^1.0.5",
    "sass-loader": "^13.2.0",
    "storybook": "^7.0.18",
    "storybook-addon-breakpoints": "^1.0.0",
    "style-loader": "^3.3.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "tsconfig-paths-webpack-plugin": "^4.0.1",
    "tsx": "^3.12.2",
    "typescript": "~4.9.4",
    "webpack-config-dump-plugin": "^3.0.1",
    "yaml-loader": "^0.8.0"
  },
  "scripts": {
    "bootstrap": "ts-node --project tsconfig.scripts.json scripts/bootstrap.ts && graphql-let",
    "build": "npm-run-all --serial bootstrap next:build",
    "check-types": "tsc --noemit",
    "graphql:update": "ts-node --project tsconfig.scripts.json ./scripts/fetch-graphql-introspection-data.ts",
    "jss": "jss",
    "lint": "eslint && npm run check-types",
    "next:build": "next build",
    "next:dev": "cross-env NODE_OPTIONS='--inspect' APP_ENV=local next dev",
    "start": "NODE_ENV=production node server.js",
    "start:development": "node server.js",
    "start:qa": "NODE_ENV=staging APP_ENV=qa node server.js",
    "start:production:": "NODE_ENV=production APP_ENV=prod node server.js",
    "next:start": "next start",
    "scaffold": "ts-node --project tsconfig.scripts.json scripts/scaffold-component.ts",
    "start:connected": "npm-run-all --serial bootstrap --parallel next:dev start:watch-components",
    "start:connected_continue-on-error": "npm run bootstrap & npm-run-all --parallel next:dev start:watch-components",
    "start:watch-components": "ts-node --project tsconfig.scripts.json scripts/generate-component-factory.ts --watch",
    "storybook": "storybook dev -p 6006",
    "storybook:sc": "cross-env GRAPH_QL_ENDPOINT=https://cm.demosite.localhost/sitecore/api/graph/edge SITECORE_API_HOST=https://cm.demosite.localhost tsx ./scripts/generate-storybook-layout-data.ts && npm run storybook",
    "build-storybook": "storybook build",
    "plop": "plop"
  },
  "browserslist": [
    "defaults and supports css-grid"
  ]
}
