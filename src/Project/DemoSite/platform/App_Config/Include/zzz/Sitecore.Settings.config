<?xml version="1.0"?>
<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/"
               xmlns:set="http://www.sitecore.net/xmlconfig/set/"
               xmlns:role="http://www.sitecore.net/xmlconfig/role/"
               xmlns:env="http://www.sitecore.net/xmlconfig/env/">
  <sitecore>
    <settings>
      <!--<setting name="overridenSitecoreSetting" set:value="new value" />-->
      <setting name="WebStylesheet"
               set:value="/richtext/default.css?v=1.11"
               role:require="Standalone or ContentManagement" />
      <setting name="HtmlEditor.DefaultProfile"
               set:value="/sitecore/system/Settings/Html Editor Profiles/HP Rich Text Component"
               role:require="Standalone or ContentManagement" />
      <setting name="Preview.DefaultSite"
               set:value="demosite"
               role:require="Standalone or ContentManagement" />
      <setting name="JavaScriptServices.ViewEngine.Http.JssEditingSecret"
               set:value="sa4i80onlkotGtWImpQFfjEr9diIdRB82iJVv3NOcKFMhrwnqeu8LGMVUZoda0bn"
               env:require="!Local" />
      <setting name="RequestErrors.UseServerSideRedirect" set:value="true"/>
      <setting name="TranslationToggleOtherLanguage"
               set:value="es"
               role:require="Standalone or ContentManagement" />
      <setting name="ItemNameValidation" value="^[\w\*\$][\w\s\$]*(\(\d{1,}\)){0,1}$"/>
      <setting name="ExperienceEdge.EnableItemLanguageFallback" set:value="true" />
    </settings>
  </sitecore>
</configuration>