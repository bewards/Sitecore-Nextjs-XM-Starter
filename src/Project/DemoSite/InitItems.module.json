{
  "namespace": "InitItems",
  "references": ["Foundation.*", "Feature.*"],
  "items": {
    "includes": [
      {
        "name": "content-root",
        "path": "/sitecore/content",
        "scope": "singleItem",
        "allowedPushOperations": "createAndUpdate"
      },
      {
        "name": "api-key",
        "path": "/sitecore/system/Settings/Services/API Keys/DemoSite",
        "scope": "singleItem"
      },
      {
        "name": "rendering-host",
        "path": "/sitecore/system/Settings/Services/Rendering Hosts/Default",
        "scope": "singleItem",
        "allowedPushOperations": "createAndUpdate"
      },
      {
        "name": "preview-publishing-target",
        "path": "/sitecore/system/Publishing targets/Preview",
        "scope": "singleItem",
        "allowedPushOperations": "createAndUpdate"
      }
    ]
  }
}
